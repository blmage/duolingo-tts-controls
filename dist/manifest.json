{
  "name": "Duolingo TTS Controls",
  "version": "1.1.1",
  "description": "Provides playback controls for the listening challenges on Duolingo.",
  "permissions": [
    "activeTab",
    "https://*.duolingo.com/"
  ],
  "content_scripts": [
    {
      "matches": [ "https://*.duolingo.com/*" ],
      "js": [ "src/content.js" ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "src/ui.js",
    "assets/css/ui.css",
    "assets/images/close.svg",
    "assets/images/pause.svg",
    "assets/images/pin.svg",
    "assets/images/play.svg",
    "assets/images/rate-max.svg",
    "assets/images/rate-min.svg",
    "assets/images/time-max.svg",
    "assets/images/time-min.svg",
    "assets/images/settings.svg",
    "assets/images/stop.svg",
    "assets/images/volume-max.svg",
    "assets/images/volume-min.svg"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png",
    "256": "icons/icon_256.png",
    "512": "icons/icon_512.png"
  },
  "manifest_version": 2
}
